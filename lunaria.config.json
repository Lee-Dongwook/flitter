{
  "$schema": "./node_modules/@lunariajs/core/config.schema.json",
  "repository": {
    "name": "meursyphus/flitter",
    "branch": "latest",
    "rootDir": "."
  },
  "defaultLocale": {
    "label": "English",
    "lang": "en"
  },
  "locales": [
    {
      "label": "English",
      "lang": "en"
    },
    {
      "label": "한국어",
      "lang": "ko"
    },
    {
      "label": "简体中文",
      "lang": "cn"
    },
    {
      "label": "日本語",
      "lang": "ja"
    }
  ],
  "files": [
    {
      "location": "packages/docs/src/content/tutorial/**/*.mdx",
      "pattern": "packages/docs/src/content/tutorial/@lang/@path",
      "type": "universal"
    },
    {
      "location": "packages/docs/src/content/docs/**/*.mdx",
      "pattern": "packages/docs/src/content/docs/@lang/@path",
      "type": "universal"
    }
  ]
}
